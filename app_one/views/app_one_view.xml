        />
        <menuitem id="wb_app_one_menu_Sales"
                  action="wb_app_one_action_Sales"
                  name="Sales"
                  sequence="1"
                  parent="wb_app_one_root_menu"
        />
        <menuitem id="wb_app_one_menu_Inventory"
